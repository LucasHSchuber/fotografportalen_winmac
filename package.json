{
  "name": "photographerportal",
  "productName": "Photographer Portal",
  "version": "1.0.3",
  "description": "Photographer Portal Application",
  "main": "./out/main/index.js",
  "author": "Photographer Portal",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "rebuild": "electron-rebuild -f -w sqlite3 --verbose",
    "clean": "rimraf dist",
    "tag": "git tag $(node -pe \"require('./package.json').version\") && git push --tags",
    "build:win": "npm run clean && npm run build && electron-builder --win --config electron-builder.yml -p always && node post-build.js",
    "build:win:test": "npm run clean && npm run build && electron-builder --win --config electron-builder.yml -p never && node post-build.js",
    "build:mac": "npm run build && electron-builder --mac --config electron-builder.yml -p always",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@octokit/rest": "^21.0.2",
    "apexcharts": "^3.54.1",
    "axios": "^1.6.8",
    "basic-ftp": "^5.0.5",
    "bcrypt": "^5.1.1",
    "bootstrap": "^5.3.3",
    "chart.js": "^4.4.2",
    "core-js": "^3.37.1",
    "cors": "^2.8.5",
    "dompurify": "^3.1.5",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^5.1.2",
    "electron-updater": "^6.1.8",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "jquery": "^3.7.1",
    "path-browserify": "^1.0.1",
    "pdfjs-dist": "^3.4.120",
    "react-apexcharts": "^1.5.0",
    "react-bootstrap": "^2.10.2",
    "react-chartjs-2": "^5.2.0",
    "react-loader-spinner": "^6.1.6",
    "react-select2": "^4.0.3",
    "recharts": "^2.12.5",
    "select2": "^4.1.0-rc.0",
    "semver": "^7.6.2",
    "sqlite3": "^5.1.7",
    "sweetalert2": "^11.11.1",
    "sweetalert2-react-content": "^5.0.7",
    "tus-js-client": "^4.3.1",
    "vite-plugin-env-compatible": "^2.0.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.1",
    "@electron-toolkit/eslint-config-prettier": "^1.0.1",
    "@electron/rebuild": "^3.6.0",
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/page-navigation": "^3.7.0",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.8",
    "dotenv": "^16.4.7",
    "electron": "^25.6.0",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9",
    "electron-reloader": "^1.2.3",
    "electron-vite": "^1.0.28",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "postcss": "^8.4.16",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.17.0",
    "react-select": "^5.8.0",
    "react-toastify": "^9.1.3",
    "tailwindcss": "^3.1.8",
    "vite": "^4.4.9",
    "ws": "^8.14.2"
  }
}
